{
  "name": "aurelia-jspm",
  "version": "1.0.0",
  "description": "aurelia with jspm from scratch",
  "main": "index.js",
  "scripts": {
    "clean": "gulp clean",
    "bundle": "gulp bundle",
    "inject": "gulp inject",
    "unbundle": "gulp unbundle",
